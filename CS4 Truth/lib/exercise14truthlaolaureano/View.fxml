<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lib.exercise14truthlaolaureano.Exer13Controller">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <Label layoutX="259.0" layoutY="85.0" text="Subject:" />
            <Label fx:id="currentSubject" layoutX="343.0" layoutY="85.0" />
            <ImageView fx:id="currentImg" fitHeight="150.0" fitWidth="200.0" layoutX="220.0" layoutY="102.0" pickOnBounds="true" preserveRatio="true" />
            <Label layoutX="220.0" layoutY="260.0" text="Unit:" />
            <Label fx:id="currentUnits" layoutX="380.0" layoutY="260.0" />
            <Label layoutX="219.0" layoutY="285.0" text="Grades:" />
            <Label fx:id="currentGrades" layoutX="380.0" layoutY="285.0" />
            <Button fx:id="nextButton" layoutX="343.0" layoutY="328.0" mnemonicParsing="false" onAction="#nextSubject" text="Next" />
            <Button fx:id="prevButton" layoutX="233.0" layoutY="328.0" mnemonicParsing="false" onAction="#prevSubject" text="Previous" />
            <TextField fx:id="searchText" layoutX="239.0" layoutY="41.0" onAction="#searchSubj" promptText="Search..." />
            <Button fx:id="searchButton" layoutX="365.0" layoutY="41.0" onAction= "#searchSubj" mnemonicParsing="false" prefHeight="25.0" prefWidth="25.0" />
            <Label fx:id="notFound" layoutX="300.0" layoutY="24.0">
               <font>
                  <Font size="10.0" />
               </font>
            </Label>
      </children>
    </AnchorPane>
  </children>
</VBox>
